{
  "name": "@itu/tsmonads",
  "version": "4.1.0",
  "license": "MIT",
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.10",
  },
  "exports": {
    ".": "./monads.ts"
  },
  "jsr": {

  },
  "publish": {
    "exclude": [
      "spec", "example", "docs", ".vscode"
    ]
  },
  "tasks": {
    "test": "deno test spec/*",
    "check": "deno check monads.ts src/*.ts",
    "build:npm": "deno run -A build_npm.ts"
  },
  "fmt": {
    "files": {
      "include": ["monads.ts", "src/", "spec/", "example/"]
    }
  },
  "lint": {
    "files": {
      "include": ["monads.ts", "src/", "spec/", "example/"]
    }
  },
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true
  }
}